{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jquery": true
  },
  "plugins": ["security"],
  "extends": ["eslint:recommended", "plugin:security/recommended"],
  "ignorePatterns": ["temp.js", "**/dist/*.js"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": ["next", "exErr"] }]
  }
}
